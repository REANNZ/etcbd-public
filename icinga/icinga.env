ICINGA_DB_USER=icinga
ICINGA_DB_PASSWORD=icinga-db-password
ICINGA_DB_NAME=icinga
ICINGA_DB_HOST=postgres-icinga
ICINGAWEB2_DB_USER=icingaweb2
ICINGAWEB2_DB_PASSWORD=icingaweb2-db-password
ICINGAWEB2_DB_NAME=icingaweb2
ICINGAWEB2_DB_HOST=postgres-icinga
ICINGAWEB2_ADMIN_USER=admin
ICINGAWEB2_ADMIN_PASSWORD=admin-password
ICINGA_ADMIN_EMAIL=admin@example.org
EMAIL_HOST=smtp.example.org
EMAIL_PORT=25
EMAIL_USER=
EMAIL_PASSWORD=
EMAIL_USE_TLS=
EMAIL_FROM=donotreply@example.org
POSTGRES_PASSWORD=postgres-password
SITE_PUBLIC_HOSTNAME=eduroam-monitoring.example.org

# Re-notification interval in seconds.  0 disables re-notification.  Defaults to 7200 (2 hours).
ICINGA_NOTIFICATION_INTERVAL=7200

# Check interval in seconds (HARD state).  Defaults to 300 (5 minutes)
ICINGA_CHECK_INTERVAL=300

# Interval (in seconds) for rechecking after a state change (SOFT state).  Defaults to 60 seconds.
ICINGA_RECHECK_INTERVAL=60

# Parameters for fetching monitoring configuration generated by Admintool
CONF_URL_LIST=https://eduroam-admin.example.org/icingaconf
CONF_URL_USER=confuser
CONF_URL_PASSWORD=confuser-password
CONF_REFRESH_INTERVAL=3600
WGET_EXTRA_OPTS=
#--ca-certificate=/etc/icinga2/externalconf/server.crt
#--no-check-certificate
